{
  "name": "tslfx",
  "version": "0.6.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "next dev --webpack",
    "build": "next build --webpack",
    "start": "serve out",
    "lint": "eslint .",
    "tsup": "tsup --tsconfig tsup.tsconfig.json",
    "pub": "bun run tsup && npm publish",
    "format": "prettier --write .",
    "prepare": "husky"
  },
  "tsup": {
    "entry": [
      "src/shaders/index.ts"
    ],
    "clean": true,
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "three": ">=0.182.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.3",
    "@react-three/drei": "10.7.7",
    "@react-three/fiber": "9.4.2",
    "@types/node": "25.0.0",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@types/three": "0.182.0",
    "eslint": "9.39.1",
    "eslint-config-next": "16.0.8",
    "husky": "9.1.7",
    "leva": "0.10.1",
    "next": "16.0.8",
    "postcss": "8.5.6",
    "prettier": "3.7.4",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "serve": "14.2.5",
    "three": "0.182.0",
    "tailwindcss": "3.4.17",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "zustand": "5.0.9"
  },
  "overrides": {
    "three": "0.182.0"
  },
  "patchedDependencies": {
    "three@0.182.0": "patches/three@0.182.0.patch"
  }
}
